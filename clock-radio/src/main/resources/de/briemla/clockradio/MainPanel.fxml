<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import de.briemla.clockradio.ViewSwitcher?>
<?import de.briemla.clockradio.Alarm?>
<?scenebuilder-classpath-element ../../../../../../target/classes?>

<fx:root prefHeight="600.0" prefWidth="800.0" type="VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane maxHeight="50.0" prefHeight="50.0">
         <children>
            <Button mnemonicParsing="false" onAction="#openSettings" prefHeight="50.0" prefWidth="100.0" text="Einstellungen" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
      <ViewSwitcher fx:id="viewSwitch" maxHeight="500.0" prefHeight="500.0" prefWidth="800.0"/>
      <Alarm fx:id="alarm"/>
   </children>
</fx:root>
